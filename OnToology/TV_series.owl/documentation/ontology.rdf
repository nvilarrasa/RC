        <rdfs:comment xml:lang="en">Metacritic review aggregator, among different information regarding television series and other multimedia formats, has also ratings and fan and critical reviews.</rdfs:comment>
        <rdfs:label xml:lang="en">Metacritic</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Online_databases -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Online_databases">
