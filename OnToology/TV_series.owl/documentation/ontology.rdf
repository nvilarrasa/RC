        <rdfs:label xml:lang="en">Voice_Actor</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Writer -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Writer">
