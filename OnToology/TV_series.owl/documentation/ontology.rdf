    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#directed -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#directed">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#directed_by"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Director"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode"/>
        <rdfs:comment xml:lang="en">The director directed that episode.</rdfs:comment>
        <rdfs:label xml:lang="en">directed</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#directed_by -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#directed_by">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Director"/>
        <rdfs:comment xml:lang="en">The episode was directed by that director/s.</rdfs:comment>
        <rdfs:label xml:lang="en">directed_by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#distributed_by -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#distributed_by">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Season"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Distribution"/>
        <rdfs:comment xml:lang="en">Channels, platforms or methods in which the season has been distributed.</rdfs:comment>
        <rdfs:label xml:lang="en">distributed_by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#dubbed_in -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#dubbed_in">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Season"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Language"/>
        <rdfs:comment xml:lang="en">Language in which a season has been dubbed.</rdfs:comment>
        <rdfs:label xml:lang="en">dubbed_in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#has_episode -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#has_episode">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#is_in_season"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Season"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode"/>
        <rdfs:comment xml:lang="en">Episodes that are included in a season.</rdfs:comment>
        <rdfs:label xml:lang="en">has_episode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#has_rating -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#has_rating">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Season"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Review_rating"/>
        <rdfs:comment xml:lang="en">Rating given by a critical review to a tv series season.</rdfs:comment>
        <rdfs:label xml:lang="en">has_rating</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#has_season -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#has_season">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#is_in_series"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Series"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Season"/>
        <rdfs:comment xml:lang="en">Seasons included in a series</rdfs:comment>
        <rdfs:label xml:lang="en">has_season</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#is_in_season -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#is_in_season">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Season"/>
        <rdfs:comment xml:lang="en">Season where the episode is included.</rdfs:comment>
        <rdfs:label xml:lang="en">is_in_season</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#is_in_series -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#is_in_series">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Season"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Series"/>
        <rdfs:comment xml:lang="en">Series where the season is included.</rdfs:comment>
        <rdfs:label xml:lang="en">is_in_series</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#language_filmed_in -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#language_filmed_in">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Season"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Language"/>
        <rdfs:comment xml:lang="en">Original language in which the season was filmed.</rdfs:comment>
        <rdfs:label xml:lang="en">language_filmed_in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#person_awarded -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#person_awarded">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Award"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Person"/>
        <rdfs:comment xml:lang="en">The person who has received the award.</rdfs:comment>
        <rdfs:label xml:lang="en">person_awarded</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#played -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#played">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#played_by"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Actor"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode"/>
        <rdfs:comment xml:lang="en">Actor or actress that played in that episode.</rdfs:comment>
        <rdfs:label xml:lang="en">played</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#played_by -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#played_by">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Actor"/>
        <rdfs:comment xml:lang="en">Actor/s that played in that episode.</rdfs:comment>
        <rdfs:label xml:lang="en">played_by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#season_awarded_with -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#season_awarded_with">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Season"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Award"/>
        <rdfs:comment xml:lang="en">Awards the season has been awarded.</rdfs:comment>
        <rdfs:label xml:lang="en">season_awarded_with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#series_awarded_with -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#series_awarded_with">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Series"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Award"/>
        <rdfs:comment xml:lang="en">Awards that  the series has been awarded.</rdfs:comment>
        <rdfs:label xml:lang="en">series_awarded_with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#written_by -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#written_by">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#wrote"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Writer"/>
        <rdfs:comment xml:lang="en">The script of an episode was written by that writter/s.</rdfs:comment>
        <rdfs:label xml:lang="en">written_by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#wrote -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#wrote">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Writer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode"/>
        <rdfs:comment xml:lang="en">A writer wrote the script of that episode.</rdfs:comment>
        <rdfs:label xml:lang="en">wrote</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#has_rating_value -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#has_rating_value">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Review_rating"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">Every review rating has an integer value.</rdfs:comment>
        <rdfs:label>has_rating_value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#is_female -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#is_female">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Every Person can be female or not.</rdfs:comment>
        <rdfs:label>is_female</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#is_male -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#is_male">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Every Person can be male or not.</rdfs:comment>
        <rdfs:label>is_male</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#was_released -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#was_released">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Date when the episode of the series was released.</rdfs:comment>
        <rdfs:label>was_released</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Actor -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Actor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Person"/>
        <rdfs:comment xml:lang="en">A person who plays the part of a character in the series.</rdfs:comment>
        <rdfs:label xml:lang="en">Actor</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Award -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Award">
        <rdfs:comment>An award is given to a recipient as a token of recognition of excellence in a certain field. The recipient is often a single person or a representative of a group of people,</rdfs:comment>
        <rdfs:label>Award</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Best_Actor -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Best_Actor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Award"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#person_awarded"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Actor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An award given to an actor who has delivered an outstanding performance in a leading role in a series (episode or season in general) released that year.</rdfs:comment>
        <rdfs:label xml:lang="en">Best_Actor</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Best_Actress -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Best_Actress">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Award"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#person_awarded"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Actor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An award given to an actress who has delivered an outstanding performance in a leading role in a series (episode or season in general) released that year.</rdfs:comment>
        <rdfs:label xml:lang="en">Best_Actress</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Best_Series -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Best_Series">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Award"/>
        <rdfs:comment xml:lang="en">Award given to the best series as a whole. Usually an annual award that is given to a Season realeased the same year.</rdfs:comment>
        <rdfs:label xml:lang="en">Best_Series</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Best_Writing -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Best_Writing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Award"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#person_awarded"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Writer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Awarded to the writer of a screenplay (episode and or season of the series)  adapted from another source (novel or play usually) or  to the writer of an script not based on previously published material, and released that year.</rdfs:comment>
        <rdfs:label xml:lang="en">Best_Writing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Composer -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Composer">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Person"/>
        <rdfs:comment xml:lang="en">A person who writes music,  as a professional occupation, that in this ontology specifically composed the music for the series.</rdfs:comment>
        <rdfs:label xml:lang="en">Composer</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Director -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Director">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Person"/>
        <rdfs:comment xml:lang="en">A person who is in charge of the series episode and supervises the actors, the staff and key artistical decisions.</rdfs:comment>
        <rdfs:label xml:lang="en">Director</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Distribution -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Distribution">
        <rdfs:comment xml:lang="en">the channels used for sharing the series content out among their (possible) viewers.</rdfs:comment>
        <rdfs:label xml:lang="en">Distribution</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Episode">
        <rdfs:comment xml:lang="en">An episode is a coherent narrative unit within the larger dramatic work, used in this case to describe units of series that are broadcasted separately in order to form one longer series.</rdfs:comment>
        <rdfs:label xml:lang="en">Episode</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#IMDB -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#IMDB">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Online_databases"/>
