        <rdfs:comment xml:lang="en">The Washington Pownt newspaper publishes reviews of series from different critics .</rdfs:comment>
        <rdfs:label xml:lang="en">The_Washington_Post</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Voice_actor -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Voice_actor">
