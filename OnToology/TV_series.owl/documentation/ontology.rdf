        <rdfs:label xml:lang="en">IMDB</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Journalistic_criticism -->


    <owl:Class rdf:about="http://www.semanticweb.org/nona/ontologies/2022/3/TV_series#Journalistic_criticism">
